{
  "name": "Graphiql",
  "version": "1.0",
  "description": "",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "cookies",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": [],
      "js": ["content.js"],
      "all_frames": true,
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "*"
  ],
  "options_page": "options.html",
  "options_ui": {
    "chrome_style": false,
    "page": "options.html"
  },
  "browser_action": {
    "default_title": "Graphiql",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "manifest_version": 2
}
